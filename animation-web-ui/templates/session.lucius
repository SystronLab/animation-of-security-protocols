li {
    line-height: 2em;
    font-size: 16px
}

thead {
    padding-top: 12px;
    padding-bottom: 12px;
    text-align: left;
}

tr:hover {
    background-color: #ddd;
}

tr:nth-child(even){
    background-color: #f2f2f2;
}

td, th {
  border: 1px solid #ddd;
  padding: 8px;
}

btn {
    padding: 10px 24px;
    border-radius: 2px;
}